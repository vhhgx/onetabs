{
  "manifest_version": 3,
  "name": "OneTabs",
  "version": "1.0.0",
  "description": "保存并管理Chrome标签页",
  "action": {
    "default_icon": {
      "16": "icons/logo.png",
      "48": "icons/logo.png",
      "128": "icons/logo.png"
    },
    "default_title": "OneTabs"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}